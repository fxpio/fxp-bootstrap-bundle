{
    "name" : "sonatra/bootstrap-bundle",
    "type" : "symfony-bundle",
    "description" : "Twig DSL for construct a Twitter Bootstrap interface",
    "keywords" : ["twitter bootstrap 3", "block", "dsl", "template"],
    "homepage" : "https://github.com/sonatra/SonatraBootstrapBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "FranÃ§ois Pluchino",
            "email": "francois.pluchino@sonatra.com"
        }
    ],
    "require" : {
        "php" : ">=5.5.0",
        "symfony/framework-bundle" : "~3.0",
        "symfony/assetic-bundle": "~2.7",
        "kriswallsmith/assetic": "~1.3",
        "fxp/require-asset-bundle" : "1.0.*@dev",
        "fxp/require-asset" : "1.0.*@dev",
        "sonatra/block-bundle" : "1.0.*@dev"
    },
    "require-dev" : {
        "symfony/console" : "~3.0",
        "symfony/finder" : "~3.0",
        "symfony/filesystem" : "~3.0",
        "doctrine/orm": "~2.5",
        "gedmo/doctrine-extensions": "~2.4"
    },
    "suggest": {
        "bower-asset/html5shiv": "v3.7.*",
        "bower-asset/respond": "v1.4.*",
        "bower-asset/bootstrap": "v3.3.*",
        "fxp/composer-asset-plugin": "For manage NPM/Bower dependencies with Composer"
    },
    "autoload": {
        "psr-4": { "Sonatra\\Bundle\\BootstrapBundle\\" : "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.0-dev"
        }
    }
}
