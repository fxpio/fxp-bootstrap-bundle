{
    "name" : "fxp/bootstrap-bundle",
    "type" : "symfony-bundle",
    "description" : "Twig DSL for construct a Twitter Bootstrap interface",
    "keywords" : ["twitter bootstrap 3", "block", "dsl", "template"],
    "homepage" : "https://github.com/fxpio/fxp-bootstrap-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Fran√ßois Pluchino",
            "email": "francois.pluchino@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require" : {
        "php" : "^7.1.0",
        "symfony/framework-bundle" : "^4.1.0",
        "fxp/block-bundle" : "^1.0.0",
		"fxp/bootstrap" : "^1.0.0"
    },
    "require-dev" : {
        "symfony/console" : "^4.1.0",
        "symfony/finder" : "^4.1.0",
        "symfony/filesystem" : "^4.1.0",
        "doctrine/orm": "^2.6.0",
        "gedmo/doctrine-extensions": "^2.4.0",
        "fxp/require-asset-bundle" : "^1.0.0",
        "foxy/foxy": "^1.0.0"
    },
    "config": {
        "foxy": {
            "enabled": false
        }
    },
    "autoload": {
        "psr-4": { "Fxp\\Bundle\\BootstrapBundle\\" : "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.0-dev"
        }
    }
}
